# UE5 Development Configuration
# This file contains configuration options for the UE5 setup scripts
# Copy this file to .env and modify as needed

# Workspace directory where UE5 will be installed
# Default: /workspace
WORKSPACE_DIR=/workspace

# UE5 repository URL
# Default: https://github.com/EpicGames/UnrealEngine.git
UE5_REPO_URL=https://github.com/EpicGames/UnrealEngine.git

# UE5 branch to use
# Options: ue5-main (latest), release (stable), 5.3, 5.4, etc.
# Default: ue5-main
UE5_BRANCH=ue5-main

# Number of parallel build jobs
# Default: auto-detect based on CPU cores
# Set to specific number to limit CPU usage: BUILD_JOBS=4
BUILD_JOBS=auto

# Build configuration
# Options: Debug, Development, Shipping
# Default: Development
BUILD_CONFIG=Development

# Build target platform
# Options: Linux, Win64, Mac, etc.
# Default: Linux
BUILD_PLATFORM=Linux

# Skip interactive prompts (for automated setups)
# Options: true, false
# Default: false
SKIP_PROMPTS=false

# Enable verbose output
# Options: true, false  
# Default: false
VERBOSE=false